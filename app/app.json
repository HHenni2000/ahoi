{
  "expo": {
    "name": "ahhoi",
    "slug": "ahhoi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ahoi-icon.png",
    "scheme": "ahhoi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/728eda13-ca1e-4ead-9bc1-1e6f4fedf66b"
    },
    "splash": {
      "image": "./assets/images/ahoi-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAF7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kotoro.ahoi",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ahoi-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ahoi-icon.png"
    },
    "extra": {
      "apiBaseUrl": "http://72.60.80.95:8000",
      "router": {},
      "eas": {
        "projectId": "728eda13-ca1e-4ead-9bc1-1e6f4fedf66b"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "hhenni"
  }
}
